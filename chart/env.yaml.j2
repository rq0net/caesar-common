fullnameOverride: "{{ KUBERNETES.COMMON_FULLNAME_OVERRIDE }}"

image:
  repository: {{ IMAGES.CAESAR_COMMON_TAG.split(":")[0] }}
  pullPolicy: Always
  # Overrides the image tag whose default is the chart version.
  tag: {{ IMAGES.CAESAR_COMMON_TAG.split(":")[1] }}
  
config: 
  secret_key: {{ SECRETS.SECRET_KEY }}
  debug: false

nginx:
  port: 8000

db:
  db_host: {{ POSTGRESQL.POSTGRESQL_DB_HOST }}
  db_port: {{ POSTGRESQL.POSTGRESQL_DB_PORT }}
  db_user: {{ POSTGRESQL.POSTGRESQL_DB_USER }}
  db_password: {{ POSTGRESQL.POSTGRESQL_DB_PASS }}
  db_database: {{ DATABASES.COMMON_DB_NAME }}

ingress:
  enabled: false
  annotations: {}
  hosts:
    - host: common.cwcdn.com
      paths:
      - /
      
env:
  TIME_ZONE: "{{ GLOBALS.TIME_ZONE }}"
  USE_KEYCLOAK: "true"
  ALLOWED_HOSTS: "{{ DJANGO.ALLOWED_HOSTS }}"
  ALIYUN_LOG_ACCESS_KEY: {{ ALIYUN.ALIYUN_LOG_ACCESS_KEY }}
  ALIYUN_LOG_ACCESS_SECRET: {{ ALIYUN.ALIYUN_LOG_ACCESS_SECRET }}
  ALIYUN_LOG_ENDPOINT: {{ ALIYUN.ALIYUN_LOG_ENDPOINT }}
  ALIYUN_LOG_PROJECT: {{ ALIYUN.ALIYUN_LOG_PROJECT }}
  ALIYUN_LOG_LOGSTORE: {{ ALIYUN.ALIYUN_LOG_LOGSTORE }}
  PUSHER_SERVER: "{{ PUSHER.PUSHER_SERVER }}"
  CMDB_SERVER: "{{ CMDB.CMDB_SERVER }}"
  PLAN_SERVER: "{{ PLAN.PLAN_SERVER }}"
  
  COMMON_DB_NAME: "{{ DATABASES.COMMON_DB_NAME }}"
  AWS_ACCESS_KEY_ID: {{ AWS.AWS_ACCESS_KEY_ID }}
  AWS_SECRET_ACCESS_KEY: {{ AWS.AWS_SECRET_ACCESS_KEY }}
  AWS_HOSTZONE_ID_QO4: {{ AWS.AWS_HOSTZONE_ID_QO4 }}
  AWS_HOSTZONE_ID_OH5: {{ AWS.AWS_HOSTZONE_ID_OH5 }}
  AWS_HOSTZONE_ID_IA0: {{ AWS.AWS_HOSTZONE_ID_IA0 }}
  AWS_HOSTZONE_ID_RQ1: {{ AWS.AWS_HOSTZONE_ID_RQ1 }}

  SLACK_TOKEN: "{{ SLACK.SLACK_TOKEN }}"
  TELEGRAMBOT_TOKEN: "{{ TELEGRAM.TELEGRAMBOT_TOKEN }}"
  TELEGRAMBOT_CHAT_ID: "{{ TELEGRAM.TELEGRAMBOT_TOKEN }}"
  LOG_TO_TELEGRAM_BOT_TOKEN: "{{ TELEGRAM.LOG_TO_TELEGRAM_BOT_TOKEN }}"
  LOG_TO_TELEGRAM_CHAT_ID: "{{ TELEGRAM.LOG_TO_TELEGRAM_CHAT_ID }}"
  
  REDIS_HOST: "{{ REDIS.REDIS_HOST }}"
  REDIS_HOST_ASGI: "{{ REDIS.REDIS_HOST }}"
  REDIS_PORT: "{{ REDIS.REDIS_PORT }}"
  REDIS_DATABASE: "{{ REDIS.REDIS_DATABASE_CDNDOMAIN }}"
  REDIS_DATABASE_CELERY: "{{ REDIS.REDIS_DATABASE_CELERY }}"
  REDIS_PASSWORD: "{{ REDIS.REDIS_PASSWORD }}"
  REDIS_PASSWORD_ASGI: "{{ REDIS.REDIS_PASSWORD }}"

  ELASTICSEARCH_API_HOST: "{{ ELASTICSEARCH.ELASTICSEARCH_API_HOST }}"
  ELASTICSEARCH_API_KEY: "{{ ELASTICSEARCH.ELASTICSEARCH_API_KEY }}"
  
